<p id="notice"><%= notice %></p>

<h1>User Intakes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_intakes.each do |user_intake| %>
      <tr>
        <td><%= user_intake.user %></td>
        <td><%= user_intake.product %></td>
        <td><%= link_to 'Show', user_intake %></td>
        <td><%= link_to 'Edit', edit_user_intake_path(user_intake) %></td>
        <td><%= link_to 'Destroy', user_intake, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Intake', new_user_intake_path %>
